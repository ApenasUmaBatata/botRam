const Discord = require("discord.js");

module.exports = {
  config: {
    name: "sorte",
    aliases: [""]
  },
  run: async (bot, message, args) => {
    await message.react("✅");
    var list = [
      "vazio extremo",
      "nadica de nada",
      "nada",
      "vazio extremo",
      "nadica de nada",
      "nada",
      "nada",
      "nadica de nada",
      "vazio extremo",
      "vazio extremo",
      "Parabéns você acabou de ganhar o ultra, mega, raro, master, espetacular, cargo secreto!",
      "nada",
      "nada",
      "vazio extremo",
      "nadica de nada",
      "nada"
    ];

    let altstatus = list[Math.floor(Math.random() * list.length)];
    if (altstatus === "Parabéns você acabou de ganhar o ultra, mega, raro, master, espetacular, cargo secreto!") {
      message.member.roles.add("702385209043320833");
    }

    message.channel.send(
      `Olá ${message.author} vamos ver se você está com sorte:\n ${altstatus}`
    );
  }
};
